{
	"$schema": "https://biomejs.dev/schemas/1.5.3/schema.json",
  "files": {
    "include": ["src/**/*.ts"]
  },
  "organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true
		}
	},
  "formatter": {
    "indentStyle": "space",
    "lineWidth": 120,
    "lineEnding": "lf"
  }
}

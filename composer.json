{
  "name": "naoyukik/custom-rules-for-static-code-analysis",
  "version": "2.0.0",
  "license": "MIT",
  "authors": [
    {
      "name": "naoyukik"
    }],
  "require-dev": {
    "cakephp/cakephp-codesniffer": "^5.0",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.7.2",
    "phpmd/phpmd": "@stable",
    "squizlabs/php_codesniffer": "*",
    "vimeo/psalm": "^4.27",
    "zebra-north/phpcs-short-types": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/",
      "Test\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
